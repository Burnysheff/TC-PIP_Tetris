<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<Group xmlns:fx="http://javafx.com/fxml"
       fx:controller="com.example.jigsaw.client.ClientController">

    <Label fx:id="time" translateX="10" translateY="10"/>
    <Label fx:id="timeResult" translateX="10" translateY="10"/>
    <Label fx:id="movesCount" translateX="260" translateY="350"/>
    <Label fx:id="top" translateX="50" translateY="110"/>
    <Label fx:id="error" translateX="330" translateY="70"/>
    <Label fx:id="waitName" translateX="330" translateY="300"/>
    <Label fx:id="namesLabel" translateX="10" translateY="40"/>
    <Label fx:id="overallResult" translateX="250" translateY="550"/>
    <GridPane fx:id="gridPane" translateX="80" translateY="100"/>
    <Button fx:id="stop" onAction="#stop" translateX="350" translateY="600"/>
    <Button fx:id="seeTop" onAction="#seeTop" translateX="50" translateY="80"/>
    <Button fx:id="exit" onAction="#exit" translateX="230" translateY="600"/>
    <Button fx:id="restart" onAction="#restart" translateX="450" translateY="600"/>
    <Button fx:id="ready" onAction="#sendReady" translateX="380" translateY="400"/>
    <Group fx:id="group" translateX="620" translateY="250"/>
    <TextField fx:id="textArea" translateX="350" translateY="350"/>
</Group>
